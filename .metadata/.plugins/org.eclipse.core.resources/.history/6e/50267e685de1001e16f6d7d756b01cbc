#include <stdio.h>
double calculateAandV(double a, double *surfaceArea, double *volume)

int
main(void)
{
	setvbuf(stdout, NULL,_IONBF, 0);
	double a, surfaceArea, volume;
	printf("Enter the edge length: ");
	scanf("%f", &a);

	printf(calculateAandV(a, &surfaceArea, &volume));

	return 0;
}

double calculateAandV(a, &surfaceArea, &volume)
{
	*surfaceArea = 6 * (a ** 2);
	*volume = a ** 3;

}

